@model WebStore.ViewModels.EmployeeEditViewModel

@{
    ViewData["Title"] = "Удаление сотрудника Id:";
}


<div class ='container'>
<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Сотрудник @Model.FirstName @Model.LastName</h4>
    <hr />
      
    <form asp-action="DeleteConfirmed" asp-route-id="@Model.Id">
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-action="Index">Отмена</a>
    </form>
</div>
</div>
