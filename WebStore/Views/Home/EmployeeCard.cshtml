@model WebStore.Domain.Entities.Employee

<!DOCTYPE html>
<html>
    <head>
        <meta name = "veiwport" content="width=device-width" />
        <title>
           Карточка сотрудника
        </title>
    </head>

    <body>
        <h3>
            Карточка сотрудника
        </h3>
        <table>
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Фамилия</th>
                    <th>Имя</th>
                    <th>Отчество</th>
                    <th>Возраст</th>
                    <th>Дата приема на работу</th>
                </tr>
            </thead>
            <tbody>
             
                <tr>
                    <td>@Model.Id</td>
                    <td>@Model.LastName</td>
                    <td>@Model.FirstName</td>
                    <td>@Model.Patronymic</td>
                    <td>@Model.Age</td>
                    <td>@Model.EmploymentDate</td>
                </tr>
           
        </tbody>
        </table>
        <hr/>
        <a href="~/">На главную</a>
        <a href="~/Home/employees">Вернуться в список сотрудников</a>
    </body>
</html>